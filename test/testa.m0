
extern fun printInt(i: int)

fun fibonacci(n: int): int
    if n <= 2
        return 1
    else
        return fibonacci(n-1) + fibonacci(n-2)
    end
end

fun testa(): int
    i: int
    
    i = 1
    while i <= 10
        printInt(fibonacci(i))
        i = i + 1
    loop
    
    return 42
end

